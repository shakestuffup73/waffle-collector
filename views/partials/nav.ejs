  <% if (!user) { %>
    <script defer src="https://accounts.google.com/gsi/client"></script>
  <% } %>
</head>
<body>
  <nav>
    <% if (user) { %>
      <a href="/auth/logout">
        <button type="submit">Logout</button>
      </a>
      <a href="/waffles/new">Add a Waffle</a>
      <a href="/waffles">Show me the Waffles!</a>
      <a href="/toppings/new">Add a Topping</a>
      <a href="/toppings">Topping List</a>
    <% } else { %>
      <h1> Please login and enjoy yourself some waffles! </h1>
      <div 
      id="g_id_onload"
      data-client_id="<%= googleClientID %>"
      data-login_uri="/auth/google"
      data-auto_prompt="false"
      ></div>
      <div 
      class="g_id_signin right"
      data-type="standard"
      data-size="large"
      data-theme="outline"
      data-text="sign_in_with"
      data-shape="rectangular"
      data-logo_alignment="left"
      ></div>
    <% } %> 
  </nav>
